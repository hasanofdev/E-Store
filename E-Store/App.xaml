<Application x:Class="E_Store.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:E_Store"
             Startup="ApplicationStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionaries/Left Button.xaml"/>
                <ResourceDictionary Source="/Dictionaries/RightButton.xaml"/>
                <ResourceDictionary Source="/Dictionaries/ButtonTemp.xaml"/>
                <ResourceDictionary Source="/Dictionaries/NavbarButton.xaml"/>
                <ResourceDictionary Source="/Dictionaries/FlatCombo.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ToastNotifications.Messages;component/Themes/Default.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
